#!/bin/bash

a="abc"

if [ "$a" == "abc" ]; then
   echo -e " both of them are equal "
else
   echo -e " both them are not equal "  

fi
# demo on not equals string operators
if [ "$b" != "abc" ]; then
   echo -e " both of them are not equal "
fi   

if [ -z $b ]; then
   echo "variable is empty"
fi


# demo 0n else if

c=$1


if [ "$c" -eq "10" ] ; then
   echo "value of c is 10"

elif [ "$c" -eq "20" ] ; then
   echo "value of c is 20"

elif [ "$c" -eq "30" ] ; then
   echo "value of c is 30"

else 
   echo "value is not 10 or 20 or 30"

fi        
